	var arr = [{
    "id": 1,
    "Name": "Name1"
  },
  {
    "id": 1,
    "Name": "Name2"
  },
  {
    "id": 2,
    "Name": "Name3"
  }];
  
  var prop = "id";
  
  removeDuplicatesArray: function(arr, prop) {
			let obj = {};
			return Object.keys(arr.reduce((prev, next) => {
				if (!obj[next[prop]]) obj[next[prop]] = next;
				return obj;
			}, obj)).map((i) => obj[i]);
		},
  
  
  //cleaned array
  var clean = this.removeDuplicates(arr, prop);
